{
  "buildCommand": "prisma generate && next build",
  "framework": "nextjs",
  "installCommand": "npm install",
  "regions": [
    "gru1"
  ],
  "build": {
    "env": {
      "PRISMA_GENERATE_DATAPROXY": "true",
      "NEXT_PUBLIC_FORCE_DYNAMIC": "true",
      "NEXT_DISABLE_PRERENDER": "true"
    }
  }
}